{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Amazon": "Information",
            "System": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "FormatterName": "json",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss.fff",
                "UseUtcTimestamp": true,
                "JsonWriterOptions": {
                    "Indented": false
                }
            }
        }    }
}